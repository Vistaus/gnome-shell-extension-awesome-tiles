#: src/extension.js:145
#, javascript-format
msgid "Gap size is now at %d percent"
msgid_plural "Gap size is now at %d percent"
msgstr[0] "Tussenruimte: %d procent"
msgstr[1] "Tussenruimte: %d procent"

#: src/prefs-shortcut-dialog.ui:15
msgid "Press your keyboard shortcut..."
msgstr "Druk op een sneltoetsâ€¦"

#: src/prefs-shortcut-dialog.ui:24
msgid "Keyboard Shortcut"
msgstr "Sneltoets"

#: src/prefs.ui:45
msgid "Gap Between Window and Workspace"
msgstr "Ruimte tussen venster en werkblad"

#: src/prefs.ui:53
msgid "Percentage to leave out as gap when the window in focus is tiled."
msgstr "De te reserveren tussenruimte (in procenten) als een venster gefocust en getegeld is."

#: src/prefs.ui:95
msgid "Align Window to Center"
msgstr "Venster centreren"

#: src/prefs.ui:103
msgid "Shortcut to align the window in focus to center without resizing it."
msgstr "De sneltoets waarmee een gefocust venster kan worden gecentreerd."

#: src/prefs.ui:147
msgid "Increase the Gap Size"
msgstr "Tussenruimte vergroten"

#: src/prefs.ui:155
msgid "Shortcut to increase the gap size by 5."
msgstr "De sneltoets waarmee de tussenruimte met 5% kan worden vergroot."

#: src/prefs.ui:199
msgid "Decrease the Gap Size"
msgstr "Tussenruimte verkleinen"

#: src/prefs.ui:207
msgid "Shortcut to decrease the gap size by 5."
msgstr "De sneltoets waarmee de tussenruimte met 5% kan worden verkleind."

#: src/prefs.ui:251
msgid "Tile Window to Center"
msgstr "Venster centreren en tegelen"

#: src/prefs.ui:259
msgid "Shortcut to tile the window in focus to center."
msgstr "De sneltoets waarmee een gefocust venster kan worden gecentreerd en getegeld."

#: src/prefs.ui:303
msgid "Tile Window to Left"
msgstr "Venster tegelen aan linkerkant"

#: src/prefs.ui:311
msgid "Shortcut to tile the window in focus to left."
msgstr "De sneltoets waarmee een gefocust venster aan de linkerkant van het scherm kan worden getegeld."

#: src/prefs.ui:355
msgid "Tile Window to Right"
msgstr "Venster tegelen aan rechterkant"

#: src/prefs.ui:363
msgid "Shortcut to tile the window in focus to right."
msgstr "De sneltoets waarmee een gefocust venster aan de rechterkant van het scherm kan worden getegeld."

#: src/prefs.ui:407
msgid "Tile Window to Top"
msgstr "Venster bovenaan tegelen"

#: src/prefs.ui:415
msgid "Shortcut to tile the window in focus to top."
msgstr "De sneltoets waarmee een gefocust venster bovenaan het scherm kan worden getegeld."

#: src/prefs.ui:459
msgid "Tile Window to Bottom"
msgstr "Venster onderaan tegelen"

#: src/prefs.ui:467
msgid "Shortcut to tile the window in focus to bottom."
msgstr "De sneltoets waarmee een gefocust venster onderaan het scherm kan worden getegeld."

#: src/prefs.ui:511
msgid "Tile Window to Top Left"
msgstr "Venster linksboven tegelen"

#: src/prefs.ui:519
msgid "Shortcut to tile the window in focus to top left."
msgstr "De sneltoets waarmee een gefocust venster linksboven op het scherm kan worden getegeld."

#: src/prefs.ui:563
msgid "Tile Window to Top Right"
msgstr "Venster rechtsboven tegelen"

#: src/prefs.ui:571
msgid "Shortcut to tile the window in focus to top right."
msgstr "De sneltoets waarmee een gefocust venster rechtsboven op het scherm kan worden getegeld."

#: src/prefs.ui:615
msgid "Tile Window to Bottom Left"
msgstr "Venster linksonder tegelen"

#: src/prefs.ui:623
msgid "Shortcut to tile the window in focus to bottom left."
msgstr "De sneltoets waarmee een gefocust venster linksonder op het scherm kan worden getegeld."

#: src/prefs.ui:667
msgid "Tile Window to Bottom Right"
msgstr "Venster rechtsonder tegelen"

#: src/prefs.ui:675
msgid "Shortcut to tile the window in focus to bottom right."
msgstr "De sneltoets waarmee een gefocust venster rechtsonder op het scherm kan worden getegeld."
